{
  "name": "restash",
  "version": "0.1.24",
  "description": "Simple React context, Redux alternative.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build": "npm run clean && npm run build:ts",
    "watch": "tsc -b -w",
    "start": "webpack serve --config ./build/webpack",
    "docs": "typedoc ./src --options ./typedoc.json --out ./docs && touch ./docs/.nojekyll",
    "prep": "npm run build && npm run docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blujedis/restash.git"
  },
  "files": [
    "dist",
    "docs",
    "CHANGE.md",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "React",
    "Context",
    "Redux",
    "Consumer",
    "cache",
    "Provider"
  ],
  "author": "Blujedis <blujedicorp@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blujedis/restash/issues"
  },
  "homepage": "https://github.com/blujedis/restash#readme",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "wouter": "^2.7.4"
  },
  "dependencies": {
    "dot-prop": "^6.0.1",
    "lodash.isequal": "^4.5.0"
  }
}