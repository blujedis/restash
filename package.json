    "start": "webpack-dev-server --config ./build/webpack --watch",
    "docs": "typedoc ./src --options ./typedoc.json --out ./docs && touch ./docs/.nojekyll",
    "prep": "npm run build && npm run docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
